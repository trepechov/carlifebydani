{
    "plugins": [
        "@prettier/plugin-php",
        "prettier-plugin-tailwindcss"
    ],
    "semi": true,
    "tabWidth": 4,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "none",
    "overrides": [
        {
            "files": "*.php",
            "options": {
                "parser": "html"
            }
        }
    ]
}